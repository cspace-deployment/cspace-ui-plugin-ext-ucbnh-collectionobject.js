{
  "name": "cspace-ui-plugin-ext-ucbnh-collectionobject",
  "version": "2.0.1",
  "description": "UC Berkeley Natural History Collection Object extension plugin for the CollectionSpace UI",
  "author": "Ray Lee <rhlee@berkeley.edu>",
  "license": "ECL-2.0",
  "keywords": [
    "collectionspace",
    "cspace",
    "ui",
    "plugin",
    "extension",
    "uc berkeley natural history collection object"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cspace-deployment/cspace-ui-plugin-ext-ucbnh-collectionobject.js"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-umd",
    "build-cjs": "cross-env BABEL_ENV=cjs babel ./src -d lib",
    "build-umd": "npm run build-umd-dev && npm run build-umd-prod",
    "build-umd-dev": "webpack",
    "build-umd-prod": "cross-env NODE_ENV=production webpack",
    "check": "npm run lint && npm run test && npm run coverage",
    "clean": "rimraf build lib dist coverage errorShots",
    "coverage": "istanbul report --root coverage text && istanbul report --root coverage lcov",
    "devserver": "webpack-dev-server",
    "lint": "eslint --ext .jsx,.js *.js src test examples",
    "prepare": "npm run clean && npm run build",
    "start": "cross-env NODE_ENV=test karma start",
    "test": "cross-env NODE_ENV=test karma start --singleRun=true",
    "test-selenium": "wdio wdio.conf.js"
  },
  "dependencies": {
    "cspace-ui-plugin-ext-locality": "^0.0.5",
    "react-intl": "^2.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^3.0.2",
    "cross-env": "^2.0.0",
    "cspace-ui": "^1.0.0-rc.52",
    "css-loader": "^0.28.9",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "karma": "^3.1.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "loglevel": "^1.6.1",
    "mocha": "^5.2.0",
    "nyc": "^11.9.0",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.0.1",
    "wdio-dot-reporter": "0.0.9",
    "wdio-mocha-framework": "^0.5.12",
    "wdio-sauce-service": "^0.4.11",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.13.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "json"
    ],
    "report-dir": "./coverage/Node.js",
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
